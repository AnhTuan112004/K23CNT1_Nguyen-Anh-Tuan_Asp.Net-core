@model NatLab06.Models.NatEmployee
@{
    ViewData["Title"] = "Thêm nhân viên";
}
<h2>Thêm nhân viên</h2>
<form asp-action="NatCreateSubmit" method="post">
    <div class="form-group">
        <label>Id</label>
        <input asp-for="NatId" class="form-control" />
    </div>
    <div class="form-group">
        <label>Tên</label>
        <input asp-for="NatName" class="form-control" />
    </div>
    <div class="form-group">
        <label>Ngày sinh</label>
        <input asp-for="NatBirthDay" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label>Email</label>
        <input asp-for="NatEmail" class="form-control" />
    </div>
    <div class="form-group">
        <label>Điện thoại</label>
        <input asp-for="NatPhone" class="form-control" />
    </div>
    <div class="form-group">
        <label>Lương</label>
        <input asp-for="NatSalary" class="form-control" />
    </div>
    <div class="form-group">
        <label>Trạng thái</label>
        <input asp-for="NatStatus" type="checkbox" />
    </div>
    <button type="submit" class="btn btn-success">Lưu</button>
</form>
